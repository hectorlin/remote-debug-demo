{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/local/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-g",
                "-O0",
                "-Wall",
                "-Wextra",
                "-pedantic"
            ],
            "configurationProvider": "ms-vscode.cpptools"
        },
        {
            "name": "Linux (Release)",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/local/include"
            ],
            "defines": [
                "NDEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-O2",
                "-DNDEBUG",
                "-Wall",
                "-Wextra"
            ],
            "configurationProvider": "ms-vscode.cpptools"
        }
    ],
    "version": 4
} 